{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "interface-name": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "member-access": [
            true,
            "no-public"
        ],
        "curly": [
            true,
            "as-needed"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "variable-name": [
            true,
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "no-unused-variable": true
    },
    "rulesDirectory": []
}
